include assignment

doctype
html
    title #{fileName}

    link(href='assets/css/file.css', rel='stylesheet')

    body
        #all
            h1 #{fileName}

            // Imports
            section.imports
                h1 Imports

                ul
                    each importStatement in tree.imports
                        li #{importStatement}

            // Variables
            section.variables
                h1 Variables

                each variableDefinition in tree.variables
                    mixin assignment(variableDefinition)

            // Functions
            section.functions
                h1 Functions

                each functionDefinition in tree.functions
                    mixin assignment(functionDefinition)
